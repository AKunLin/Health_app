<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luakun.dao.CheckItemDao">
    <insert id="add" parameterType="com.luakun.pojo.CheckItem">
        insert into t_checkitem (code,name,sex,age,price,type,remark,attention) values (#{code},#{name},#{sex},#{age},#{price},#{type},#{remark},#{attention})
    </insert>
    <select id="findAll" resultType="com.luakun.pojo.CheckItem">
        select * from t_checkitem ;
    </select>
</mapper>